{
  "name": "bem-graph",
  "version": "0.0.0",
  "description": "Bem graph storage",
  "license": "MPL-2.0",
  "author": "Andrew Abramov <mail@blond.im>",
  "keywords": [
    "bem",
    "graph",
    "entity",
    "successors",
    "dependencies"
  ],
  "repository": "bem-sdk/bem-graph",
  "engines": {
    "node": ">= 4"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "dependencies": {
  },
  "devDependencies": {
    "eslint": "^3.2.2",
    "eslint-config-pedant": "^0.7.0"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc ava",
    "lint": "eslint ./lib && cd spec && eslint . && cd ../test && eslint .",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "ava": {
    "files": [
      "spec/**/*.spec.js",
      "test/**/*.test.js"
    ],
    "source": [
      "lib/**/*.js"
    ],
    "concurrency": 25
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
