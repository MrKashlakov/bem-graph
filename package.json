    "ho-iter": "blond/ho-iter",
    "lodash": "4.12.0",
    "spread-args": "^0.2.0"
  },
  "devDependencies": {
    "ava": "^0.15.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.13.1",
    "eslint-config-pedant": "^0.5.1",
    "nyc": "^6.4.4"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc ava",
    "lint": "jshint . && jscs . -c .jscs.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "ava": {
    "files": [
      "spec/**/*.spec.js",
      "test/**/*.test.js"
